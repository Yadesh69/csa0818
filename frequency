arr=list(map(int, input("Enter numbers seperated by space:").split()))
freq={}
for num in arr:
    freq[num]=freq.get(num,0)+1
print("Frequency of each element:")
for key,value in freq.items():
    print(f"{key}:{value}")
max_freq=max(freq.values())
most_frequency=[key for key,value in freq.items()if value==max_freq]
print("\n Elements with highest frequency:",most_frequency)
print("Highest frequency count:",max_freq)
